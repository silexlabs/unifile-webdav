{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "lib",
        "includePattern": ".+\\.js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "docdash": {
      "static": true,
      "sort": false
    },
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}
